<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
	android:shape="rectangle"
	>
	<!-- 线圈内颜色 -->
	<solid android:color="@color/white"/>
	<!-- 虚线颜色 -->
	<stroke android:width="1dip" android:color="@color/white"
		android:dashWidth="0dip" android:dashGap="2dip" />
	<!-- padding -->
	<padding android:left="2dip" android:top="2dip"
		android:right="2dip" android:bottom="2dip" />

	<!-- 圆角 -->
	<corners android:topLeftRadius="4dip" android:topRightRadius="4dip"
		android:bottomRightRadius="4dip" android:bottomLeftRadius="4dip" />

</shape>